<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FidelizeMainActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
        >
    <TextView android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textSize="25dp"
        />




        <Button
            android:id="@+id/bt_new_elector"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/title_act_new_elector"
            android:onClick="goToNewElector"
            />
        <Button
            android:id="@+id/bt_elector_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Eleitores"
            android:onClick="goElector"
            />
        <Button
            android:id="@+id/bt_web_elector_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Eleitores Remotos"
            android:onClick="webElector"
            />

</LinearLayout>
    </ScrollView>
</LinearLayout>
